package spring;

public class Person {
	String name;
	int age;
	Address addr;
	public Person(String name,int age,Address address){
		this.name=name;
		this.age=age;
		this.addr=addr;
	}
	public void show() {
		System.out.println(name);
		System.out.println(age);
		System.out.println(address);
	}

}
